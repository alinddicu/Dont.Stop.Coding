{
	"Framework": "jasmine",
	"Compile": {
		"Mode": "External",
		"Extensions": [ ".ts" ],
		"ExtensionsWithNoOutput": [ ".d.ts" ],
		"Executable": "compile.bat",
		"UseSourceMaps": true,
		"OutDirectory": "./generated"
	},
	"References": [
		{
			"Includes": [
				"../Dont.Stop.Coding.Web/src"
			]
		}
	],
	"Tests": [
		{
			"Includes": [ "./Tests/*.ts" ],
			"Excludes": [ "./Scripts/*.d.ts" ]
		}
	]
}